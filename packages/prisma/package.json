    "mock": "dotenv -e ../env/.env -- tsx src/mock.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/pg": "^8.16.0",
    "@workspace/env": "workspace:*",
    "dotenv-cli": "catalog:",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "catalog:"
  }
}
