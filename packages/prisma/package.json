{
  "name": "@workspace/prisma",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "migrate:dev": "dotenv -e ../env/.env -- npx prisma migrate dev",
    "migrate:deploy": "dotenv -e ../env/.env -- npx prisma migrate deploy",
